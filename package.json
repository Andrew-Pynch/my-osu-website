{
  "name": "my-osu-website",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://web.engr.oregonstate.edu/~hessro/",
  "dependencies": {
    "@emotion/core": "^10.0.4",
    "@emotion/styled": "^10.0.4",
    "markdown-it": "^8.4.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "react-scripts": "^2.1.1"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "./deploy.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
